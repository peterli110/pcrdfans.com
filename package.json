{
  "name": "pcrdfans.com",
  "version": "1.0.0",
  "description": "pcrdfans.com",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test jest",
    "test:dev": "NODE_ENV=test jest --watchAll",
    "start": "NODE_ENV=production PORT=$PORT node ./build/production-server/index.js",
    "build": "rm -rf ./build && BUNDLE_ANALYZE=both next build && tsc --project tsconfig.server.json",
    "dev": "nodemon server/index.ts",
    "export": "npm run build && next export",
    "prettier": "prettier  --write **/*.{ts,tsx,js,jsx}",
    "lint": "npm run prettier && tslint -c tslint.json -t stylish --fix {pages,src,server}/**/*.{ts,tsx}",
    "lint:scss": "stylelint --fix {src,pages}/**/*.scss",
    "lint:types": "tsc -p . --noEmit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run lint:types",
      "pre-push": "npm run test"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run lint",
      "git add"
    ],
    "*.scss": [
      "npm run lint:scss",
      "git add"
    ]
  },
  "keywords": [
    "nextjs",
    "boilerplate",
    "typeccript",
    "react",
    "redux",
    "prettier",
    "stylelint",
    "jest",
    "enzyme",
    "svg sprite",
    "husky",
    "lint staged"
  ],
  "author": "Peter Li",
  "repository": {
    "type": "git",
    "url": "https://github.com/peterli110/pcrd-server.git"
  },
  "license": "ISC",
